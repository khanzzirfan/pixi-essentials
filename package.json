{
  "name": "pixi-essentials",
  "version": "1.0.0",
  "description": "Essential plugins for PixiJS applications - Fork with modern Canva-style transformer",
  "main": "index.js",
  "scripts": {
    "prepare": "rush install",
    "build:compile": "rush build",
    "build:types": "rush build:types",
    "build": "npm run build:compile && npm run build:types",
    "prepublishOnly": "npm run build",
    "test": "cd ./tools/unit-tests && pnpm test && cd ../..",
    "publish:transformer": "cd packages/transformer && npm run publish:package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khanzzirfan/pixi-essentials.git"
  },
  "keywords": [
    "pixi.js",
    "pixi",
    "webgl",
    "renderer",
    "plugins",
    "transformer",
    "canva-style"
  ],
  "author": "Irfan Khan (khanzzirfan)",
  "contributors": [
    "Shukant K. Pal <shukantpal@outlook.com> (Original author)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khanzzirfan/pixi-essentials/issues"
  },
  "homepage": "https://github.com/khanzzirfan/pixi-essentials#readme",
  "dependencies": {
    "d-path-parser": "^1.0.0"
  },
  "packageManager": "pnpm@8.15.5+sha512.b051a32c7e695833b84926d3b29b8cca57254b589f0649d899c6e9d0edb670b91ec7e2a43459bae73759bb5ce619c3266f116bf931ce22d1ef1759a7e45aa96f"
